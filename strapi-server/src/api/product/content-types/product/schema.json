      "target": "api::category.category"
    },
    "featured": {
      "type": "boolean"
    },
    "archived": {
      "type": "boolean"
    },
    "variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::variant.variant"
    }
  }
